    "eslint": "^9.37.0",
    "prettier": "^3.3.0",
    "tsup": "^8.1.0",
    "turbo": "^2.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
