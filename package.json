{
  "name": "xrpl-connect",
  "version": "0.0.0",
  "private": true,
  "description": "A framework-agnostic wallet connection toolkit for XRPL",
  "keywords": [
    "xrpl",
    "wallet",
    "web3",
    "ripple",
    "xaman",
    "crossmark",
    "walletconnect"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/xrpl-connect"
  },
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
